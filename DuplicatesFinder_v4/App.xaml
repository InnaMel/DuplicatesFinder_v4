<Application
    x:Class="DuplicatesFinder_v4.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:DuplicatesFinder_v4"
    StartupUri="Views/MainView.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <BooleanToVisibilityConverter x:Key="BoolToVis" />
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resources/BorderCornerRadiusSTYLE.xaml" />
                <ResourceDictionary Source="/Resources/CirclesSTYLE.xaml" />
                <ResourceDictionary Source="/Resources/GridViewHeaderSTYLE.xaml" />
                <ResourceDictionary Source="/Resources/MyHeaderNameTemplate.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
